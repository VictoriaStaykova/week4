#include <iostream>
using namespace std;

int main ()
{
    int n;
    cout << "Enter n: " ;
    cin >> n;
    int a,b,c;
    a = n / 100;
    b = n / 10%10;
    c = n % 10;
    if (a*b*c >= 100)
    {
        cout << "YES" <<" "<< a*b*c - a - b - c << "." << endl;
    }
    else cout << "NO" <<" "<< a*b*c << "." << endl;
return 0;
}
